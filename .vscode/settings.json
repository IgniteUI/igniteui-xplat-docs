{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/*.js": {"when": "$(basename).ts"},
        "**/*.js.map": true
    },
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.colorCustomizations": {
        "activityBar.background": "#202D47",
        "titleBar.activeBackground": "#2D4063",
        "titleBar.activeForeground": "#F9FAFC"
    },
    "cSpell.words": [
    ]
}